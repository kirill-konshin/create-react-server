{
  "name": "create-react-server",
  "version": "0.2.0",
  "description": "Server & middleware for React + Router + Redux with Server Side Rendering",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && NODE_ENV=production webpack --progress",
    "start": "NODE_ENV=development babel-node ./server.js",
    "server": "NODE_ENV=production babel-node ./server.js"
  },
  "jest": {
    "testRegex": "(/src/.*\\.spec.js)$",
    "collectCoverage": true
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-ensure-ignore"
    ]
  },
  "main": "src/index.js",
  "bin": {
    "create-react-server": "src/server.js"
  },
  "devDependencies": {
    "babel-plugin-transform-ensure-ignore": "^0.1.0",
    "babel-preset-react-app": "^2.2.0",
    "jest": "^19.0.2",
    "memory-fs": "^0.4.1",
    "node-fetch": "^1.6.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0"
  },
  "dependencies": {
    "babel-register": "^6.24.0",
    "express": "^4.14.1",
    "hoist-non-react-statics": "^1.2.0",
    "yargs": "^7.0.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "react-router": "3.*"
  },
  "author": "Kirill Konshin",
  "repository": {
    "type": "git",
    "url": "git://github.com/kirill-konshin/rcreate-react-server.git"
  },
  "bugs": {
    "url": "https://github.com/kirill-konshin/create-react-server/issues"
  },
  "homepage": "https://github.com/kirill-konshin/create-react-server",
  "engines": {
    "node": ">=0.10.36"
  },
  "license": "MIT"
}
